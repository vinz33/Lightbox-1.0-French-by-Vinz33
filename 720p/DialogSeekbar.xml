				<info>Player.Progress</info>
				<texturebg>ProgressMidGreen.png</texturebg>
				<midtexture>ProgressLime.png</midtexture>
			</control>
			
			<control type="label">
				<description>Player Times</description>
				<posx>527</posx>
				<posy>622</posy>
